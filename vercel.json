{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/service-worker.js",
      "dest": "/dist/service-worker.js",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "src": "/manifest.webmanifest",
      "dest": "/dist/manifest.webmanifest"
    },
    {
      "src": "/static/(.*)",
      "dest": "/dist/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ]
}